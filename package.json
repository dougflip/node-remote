{
  "name": "node-remote",
  "version": "0.0.0",
  "description": "Expose unix commands via HTTP endpoints",
  "main": "src/app.coffee",
  "repository": {
    "type": "git",
    "url": "git://github.com/dougflip/node-remote.git"
  },
  "scripts": {
    "start": "coffee src/app.coffee",
    "build:client": "browserify angular/node-remote.js -o angular/bundle.js"
  },
  "author": "Doug DiFilippo (dougflip)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dougflip/node-remote/issues"
  },
  "dependencies": {
    "express": "~3.4.4",
    "jade": "~0.35.0"
  },
  "devDependencies": {
    "browserify": "~3.38.1",
    "browserify-shim": "~3.4.1",
    "chai": "~1.8.1",
    "glob": "^3.2.9",
    "gulp": "^3.6.0",
    "karma": "~0.12.4",
    "karma-chrome-launcher": "~0.1.3",
    "karma-jasmine": "~0.2.0",
    "karma-phantomjs-launcher": "~0.1.4",
    "mocha": "~1.14.0",
    "sinon": "~1.7.3",
    "vinyl-source-stream": "^0.1.1"
  },
  "browser": {
    "./public/bowercomponents/angular/angular.js": "angular",
    "./public/bowercomponents/angular-mocks/angular-mocks.js": "angular-mocks"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": "global:angular",
    "angular-mock": {
      "depends": [
        "angular"
      ],
      "exports": "global:angular.mock"
    }
  }
}
