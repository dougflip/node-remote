{
  "name": "node-remote",
  "version": "0.1.0",
  "description": "Front end client for node-remote-api",
  "main": "src/app.coffee",
  "repository": {
    "type": "git",
    "url": "git://github.com/dougflip/node-remote.git"
  },
  "scripts": {
    "postinstall": "bower install",
    "start": "node server",
    "test": "node_modules/karma/bin/karma start --single-run",
    "build": "browserify app/node-remote.js -o app/bundle.js",
    "e2e": "protractor protractor.conf.js"
  },
  "author": "Doug DiFilippo (dougflip)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dougflip/node-remote/issues"
  },
  "dependencies": {
    "envify": "^3.2.0",
    "events": "~1.0.0"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "bower": "^1.5.2",
    "browserify": "~3.38.1",
    "browserify-shim": "~3.4.1",
    "express": "~4.8.7",
    "karma": "~0.12.4",
    "karma-browserify": "0.0.6",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "~0.2.0",
    "karma-phantomjs-launcher": "~0.1.4"
  },
  "browser": {
    "./app/public/bower-components/angular/angular.js": "angular",
    "./app/public/bower-components/angular-mocks/angular-mocks.js": "angular-mocks"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim",
      "envify"
    ]
  },
  "browserify-shim": {
    "angular": "global:angular",
    "angular-mock": {
      "depends": [
        "angular"
      ],
      "exports": "global:angular.mock"
    }
  }
}
